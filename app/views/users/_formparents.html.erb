<%= form_for :register do |f| %>
  <p>
    <%= f.label "Email" %><br>
    <%= f.text_field :email %>
  </p>
 
  <p>
    <%= f.label "Mot de passe" %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.label "Confirmer le mot de passe" %><br>
    <%= f.password_field :password_confirmation %>
  </p>
 
  <p>
    <%= f.submit "Envoyer" %>
  </p>
<% end %>