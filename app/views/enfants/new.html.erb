<h1> Inscription de l'enfant </h1>
<h3>Étape 1/2</h3>

<h3>Enfant 1</h3>

<%= form_for @enfant do |f| %>
<p>
   <%= f.label "Nom" %><br>
   <%= f.text_field :nomenfant %>
</p>   
<p>
   <%= f.label "Prénom" %><br>
   <%= f.text_field :prenomenfant %>
</p> 
<p>
   <%= f.label "Date de naissance" %><br>
   <%= f.text_field :datenaissance %>
</p> 
<p>
   <%= f.label "Ecole" %><br>
   <%= f.text_field :ecole %>
</p> 
<p>
   <%= f.label "Classe" %><br>
   <%= f.text_field :classe %>
</p> 

   <%= f.submit "Envoyer" %>

<%end%>